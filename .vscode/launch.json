{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug FastAPI (uvicorn)",
        "type": "python",
        "request": "launch",
        "module": "uvicorn",
        "args": [
          "main:app",
          "--reload",
          "--host",
          "127.0.0.1",
          "--port",
          "8000"
        ],
        "jinja": true,
        "justMyCode": true,
        "console": "integratedTerminal",
        "env": {
          "PYTHONUNBUFFERED": "1"
        }
      }
    ]
  }
  